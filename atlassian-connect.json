{
  "name": "ABC Manager - Weekly Report",
  "description": "Generate weekly reports from Jira issues, categorizing child issues by status",
  "key": "com.abcreport.weekly-report",
  "baseUrl": "https://your-domain.ngrok.io",
  "vendor": {
    "name": "ABC Report",
    "url": "https://example.com"
  },
  "authentication": {
    "type": "jwt"
  },
  "apiVersion": 1,
  "modules": {
    "generalPages": [
      {
        "key": "weekly-report",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "ABC Weekly Report"
        },
        "url": "/",
        "icon": {
          "url": "/icon.png",
          "width": 24,
          "height": 24
        }
      }
    ]
  },
  "scopes": [
    "read:jira-work",
    "read:jira-user"
  ],
  "lifecycle": {
    "installed": "/lifecycle/installed",
    "uninstalled": "/lifecycle/uninstalled"
  },
  "enableLicensing": false
}

